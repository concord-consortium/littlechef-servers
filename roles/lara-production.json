{
    "name": "authoring-production",
    "chef_type": "role",
    "json_class": "Chef::Role",
    "run_list": [
        "role[lara]"
    ],
    "default_attributes": {},
    "override_attributes": {
        "cc_rails": {
            "db_rds_instance_name": "lara",
            "db_pool": 9,
            "server_name": "authoring.concord.org"
        },
        "cc_rails_portal": {
            "site_url": "http://authoring.concord.org",
            "site_name": "LARA"
        },
        "lara": {
            "sso_server_url": "http://has.portal.concord.org",
            "sso_client_id": "authoring",
            "stage": "production"
        },
        "rails": {
            "max_pool_size": 8,
            "min_instances": 2
        },
        "new_relic":{
            "rpm_account_type": "standard_key",
            "app_name": "LARA (pro)"
        }
    }
}
