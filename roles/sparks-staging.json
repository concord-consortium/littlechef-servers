{
    "name": "sparks-staging",
    "chef_type": "role",
    "json_class": "Chef::Role",
    "run_list": [
        "role[sparks-portal]"
    ],
    "default_attributes": {
    },
    "override_attributes": {
        "cc_rails_portal": {
            "db": "sparks-staging",
            "site_url": "http://aws1.sparks.concord.org",
            "rpm_name": "SPARKS staging (aws)",
            "runnable_mime_type": "run_sparks_html",
            "top_level_container_name": "page"
        },
        "http_proxies": [
            { "path": "/couchdb/learnerdata/", 
              "remote": "http://couchdb.cosmos.concord.org/sparks_data_staging_aws/",
              "options": "nocanon" },
            { "path": "/couchdb/activities/", 
              "remote": "http://couchdb.cosmos.concord.org/sparks/",
              "options": "nocanon"},
            { "path": "/sparks/qucsator/solve", 
              "remote": "http://otto.concord.org/qucs/" },
            { "path": "/sparks-content/", 
              "remote": "http://sparks.portal.concord.org/sparks-content/" }
        ]

    }
}
