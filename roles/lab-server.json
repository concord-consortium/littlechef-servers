{
    "name": "lab-server",
    "chef_type": "role",
    "json_class": "Chef::Role",
    "run_list": [
        "recipe[users::sysadmins]",
        "recipe[sudo]",
        "recipe[git]",
        "recipe[rvm_passenger::apache2]",
        "recipe[lab-server]"
    ],
    "default_attributes": {
        "authorization": {
            "sudo": {
                "passwordless": true,
                "users": [
                    "ubuntu"
                ],
                "groups": [
                    "sysadmin",
                    "admin"
                ]
            }
        },
        "nodejs": {
            "install_method": "package"
        },
        "rvm_passenger::apache2": {
          "rvm_passenger/version": "3.0.12",
          "rvm_passenger/rvm_ruby": "1.9.3-p194"
        }
    },
    "override_attributes": {}
}