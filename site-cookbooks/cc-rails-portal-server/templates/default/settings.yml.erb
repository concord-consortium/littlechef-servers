<% portal = node[:cc_rails_portal] %>
---
production:
  password_for_default_users: <%= @default_password %>
  site_name: <%= portal[:site_name] %>
  site_url: <%= portal[:site_url] %>
  authoring_site_url: <%= portal[:authoring_site_url] %>
  theme: <%= portal[:theme] %>
  admin_email: <%= portal[:email] %>
  help_email: <%= portal[:email] %>
  default_maven_jnlp:
    server: concord
    family: <%= portal[:jnlp_family] %>
    version: <%= portal[:jnlp_version] %>
  maven_jnlp_families:
  - <%= portal[:jnlp_family] %>
  maven_jnlp_servers:
  - name: concord
    host: http://jnlp.concord.org
    path: /dev3/org/concord/maven-jnlp/
  use_adhoc_workgroups: <%= portal[:use_adhoc_workgroups] %>
  site_school: <%= portal[:site_name] %>-school
  site_district: <%= portal[:site_name] %>-district
  use_gse: <%= portal[:use_gse] %>
  active_school_levels:
  - '2'
  - '3'
  - '4'
  active_grades:
  - '6'
  - '7'
  - '8'
  - '9'
  - '10'
  - '11'
  - '12'
  net_logo_package_name: otrunknl41
  <% if portal[:tiny_mce] && portal[:tiny_mce][:buttons1] %>
  tiny_mce:
    buttons1:
    <% portal[:tiny_mce][:buttons1].each do |buttons| %>
    - <%= buttons %>
    <% end %>
  <% end %>
  dont_sanitize_xhtml: <%= portal[:dont_sanitize_xhtml] %>
  <% admin_user = portal[:admin_user] %>
  default_admin_user:
    first_name: <%= admin_user[:first_name] %>
    login: <%= admin_user[:login] %>
    last_name: <%= admin_user[:last_name] %>
    email: <%= admin_user[:email] %>
  states_and_provinces: <%= portal[:states_and_provinces] %>
  runnables_use: otrunk_jnlp
  runnable_mime_type: <%= portal[:runnable_mime_type] %>
  top_level_container_name: <%= portal[:top_level_container_name] %>
  <% if portal[:recent_activity_on_login] %>
  recent_activity_on_login: <%= portal[:recent_activity_on_login] %>
  <% end %>
  pepper: <%= @site_key %>
  codap_url: http://codap.concord.org/releases/latest/
  log_manager_data_interactive_url: //cc-log-manager.herokuapp.com/di

