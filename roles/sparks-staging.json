{
    "name": "sparks-staging",
    "chef_type": "role",
    "json_class": "Chef::Role",
    "run_list": [
        "role[sparks-portal]"
    ],
    "default_attributes": {
    },
    "override_attributes": {
        "cc_rails_portal": {
            "db": "sparks-staging",
            "site_url": "http://aws1.sparks.concord.org",
            "rpm_name": "SPARKS staging (aws)",
            "runnable_mime_type": "run_sparks_html",
            "top_level_container_name": "page",
            "s3": "sparks"
        },
        "cc_couchdb": {
          "id": "sparks-staging-couchdb"
        }
    }
}

